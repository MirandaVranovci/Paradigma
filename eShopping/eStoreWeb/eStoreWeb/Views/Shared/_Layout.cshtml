                        <div class="col-xs-2">
                            <div id="colorlib-logo"><a href="index.html">Store</a></div>
                        </div>
                        <div class="col-xs-10 text-right menu-1">
                            <ul>
                                <li>
                                    <a href="SHOP/Index">
                                        Shop product
                                    </a>
                                </li>
                                <li><a href="index.html">Home</a></li>
