                <ul class="sidebar-menu tree" data-widget="tree">


                    @foreach (var item in db.SYSMENU_PARENT.OrderBy(q => q.Renditja))
                    {
                        <li class="treeview">
                            <a href="#" id="li_@item.ID">
                                <i class="@item.HtmlClass"></i><span> @item.Pershkrimi</span>
                                <span class="pull-right-container">
                                    <i class="fa fa-angle-left pull-right"></i>
                                </span>
                            </a>
                            <ul class="treeview-menu">
                                @foreach (var submenys in db.SYSMENUs.Where(q =>q.ParentID == item.ID).OrderBy(x => x.Renditja))
                                {
                                    string linkid = submenys.Controller + "_" + submenys.ActionName;

                                    <li>
                                        @if (submenys.OpParamName != null)
                                        {
                                            <a href="/@submenys.Controller/@submenys.ActionName?@submenys.OpParamName=@submenys.OpParamValue" data-id="@item.ID" id="@linkid">
                                                <i class="@submenys.HtmlClass"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @submenys.Pershkrimi
                                            </a>
                                        }
                                        else
                                        {
                                            <a href="@Url.Action(submenys.ActionName,submenys.Controller)" data-id="@item.ID" id="@linkid">
                                                <i class="@submenys.HtmlClass"></i>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; @submenys.Pershkrimi
                                            </a>
                                        }

                                    </li>

                                }
                            </ul>
                        </li>
                    }
                </ul>
            </section>
            <!-- /.sidebar -->
